{
  "expo": {
    "name": "gorilla-base",
    "description": "Gorilla Base is an online seller assistant that helps the online seller, record their day to day transactions such as order, sales, deliveries and payments.",
    "slug": "gorilla-base",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main_logo.png",
    "splash": {
      "image": "./assets/main_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blademonkey.gorilla-base"
    },
    "android": {
      "package": "com.blademonkey.gorillaBase"
    }
  }
}
